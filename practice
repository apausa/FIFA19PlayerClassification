    "checkpoint = ModelCheckpoint(\n",
    "    'mejores_pesos.weights.h5',\n",
    "    monitor='val_categorical_accuracy',\n",
    "    mode='max',\n",
    "    save_best_only=True,\n",
    "    save_weights_only=True,\n",
    "    verbose=0\n",
    ")\n",
    "\n",
    "reduce_lr = ReduceLROnPlateau(\n",
    "    'val_loss',\n",
    "    factor=0.5,\n",
    "    patience=50,\n",
    "    verbose=1\n",
    ")"
   ]
  },
