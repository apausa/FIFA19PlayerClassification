    "from sklearn.preprocessing import StandardScaler\n",
    "from sklearn.model_selection import train_test_split\n",
    "\n",
    "ATT_FILE = \"/kaggle/input/footballplayers/FootballPlayerPreparedCleanAttributes.csv\"\n",
    "LABEL_FILE = \"/kaggle/input/footballplayers/FootballPlayerOneHotEncodedClasses.csv\"\n",
    "\n",
    "scaler = StandardScaler()\n",
    "\n",
    "attributes = pd.read_csv(ATT_FILE)\n",
    "label = pd.read_csv(LABEL_FILE)\n",
    "\n",
    "# Stratified split\n",
    "y_labels = np.argmax(label.values, axis=1)\n",
    "x_temp, x_final_test, y_temp, t_final_test = train_test_split(\n",
    "    attributes.values, label.values, \n",
    "    test_size=0.2, stratify=y_labels, random_state=8\n",
    ")\n",
    "\n",
    "y_temp_labels = np.argmax(y_temp, axis=1)\n",
    "x_train, x_dev, t_train, t_dev = train_test_split(\n",
    "    x_temp, y_temp,\n",
    "    test_size=0.2, stratify=y_temp_labels, random_state=8\n",
    ")\n",
    "\n",
    "# Add normalization\n",
    "x_train = scaler.fit_transform(x_train)\n",
    "x_dev = scaler.transform(x_dev)\n",
    "x_final_test = scaler.transform(x_final_test)\n",
    "\n",
    "print (\"x_train:\",x_train.shape)\n",
    "print (\"t_train:\",t_train.shape)\n",
    "\n",
    "print (\"x_dev:\",x_dev.shape)\n",
    "print (\"t_dev:\",t_dev.shape)\n",
    "\n",
    "print (\"x_test:\",x_final_test.shape)\n",
    "print (\"t_test:\",t_final_test.shape)\n",
    "\n",
    "print(\"Training samples: \", len(x_train))\n",
    "print(\"Validation samples: \", len(x_dev))\n",
    "print(\"Testing samples: \", len(x_final_test))"
   ]
